<div>
    <h1>Quản lý người dùng bị đã xóa</h1>
    <table class="table">
    <thead>
      <tr>
        <th scope="col">Email</th>
        <th scope="col">Status</th>
        <th scope="col">Action</th>
      </tr>
    </thead>
    <tbody>
      {{#each data}}
        <tr>
          <td>{{this.email}}</td>
          <td>{{this.active}}</td>
          <td>
            <a
              type="button"
              class="btn btn-danger"
              href="/customer/destroy/{{this._id}}"
            >Xóa\</a>
            <a
              type="button"
              class="btn btn-info"
              href="/customer/restore/{{this._id}}"
            >    
            khôi phục</a>

          </td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>